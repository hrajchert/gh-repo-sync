{
  "name": "gh-repo-sync",
  "version": "0.0.2-dev",
  "description": "WIP: Helper tool to sync githubs configuration",
  "main": "output/Main/index.js",
  "directories": {
    "test": "test"
  },
  "bin": {
    "gh-repo-sync": "bin/gh-repo-sync"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hrajchert/gh-repo-sync"
  },
  "scripts": {
    "start": "node bin/gh-repo-sync",
    "start-dev": "spago run -w",
    "tdd": "spago test -w",
    "build": "spago build",
    "test": "spago test",
    "serve": "parcel web/index.html --open",
    "build-prod": "mkdir -p web-prod && cp web/index.html web-prod/ && rm -rf dist && spago bundle-app --main Web.Main --to web-prod/index.js && parcel build web-prod/index.html"
  },
  "keywords": [
    "pursescript",
    "github",
    "tool"
  ],
  "author": "Hernan Rajchert",
  "license": "MIT",
  "dependencies": {
    "request": "^2.88.2",
    "request-promise-native": "^1.0.5",
    "xhr2": "^0.1.4"
  },
  "bugs": {
    "url": "https://github.com/hrajchert/gh-repo-sync/issues"
  },
  "homepage": "https://github.com/hrajchert/gh-repo-sync",
  "devDependencies": {
    "parcel": "^1.12.4",
    "purescript": "^0.13.8",
    "purty": "^6.2.0",
    "spago": "^0.17.0"
  }
}
